#include<iostream>
using namespace std;
int main()
{
int n,i;
cout<<"enter n"<<endl;
cin>>n;
int arr[n];
cout<<"enter the array elements"<<endl;
for(i=0;i<n;i++)
{
cin>>arr[i];
}  
int k;
cout<<"enter sum"<<endl;
cin>>k;
int count=0;
int sum=0;
i=0;
int j;
for(j=0;j<n;j++)
{
sum=sum+arr[j];
while(sum>k)
{
sum=sum-arr[i];
i+=1;
}
if(sum==k)
{
count+=1;
}
}
cout<<count;
return 0;
}